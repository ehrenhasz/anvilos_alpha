{
  "name": "busybox",
  "version": "1.36.1",
  "source_url": "https://busybox.net/downloads/busybox-1.36.1.tar.bz2",
  "archive_name": "busybox-1.36.1.tar.bz2",
  "build_subdir": "busybox-src",
  "configure": "make defconfig HOSTCC='x86_64-unknown-linux-musl-gcc -static' CC='x86_64-unknown-linux-musl-gcc'",
  "post_configure_patches": [
      {
          "file": ".config",
          "replace": "# CONFIG_STATIC is not set",
          "with": "CONFIG_STATIC=y"
      },
      {
          "file": ".config",
          "replace": "CONFIG_TC=y",
          "with": "CONFIG_TC=n"
      }
  ],
  "build": "make -j$(nproc) HOSTCC='x86_64-unknown-linux-musl-gcc -static' CC='x86_64-unknown-linux-musl-gcc'",
  "install": "make install CONFIG_PREFIX={rootfs} HOSTCC='x86_64-unknown-linux-musl-gcc -static' CC='x86_64-unknown-linux-musl-gcc'"
}
