				pinctrl-single,drive-strength = <0x30 0xf0>;
			};
			uart0_cfg_func: uart0-cfg-pins {
				pinctrl-single,pins = <
					0x208 0		 
					0x20c 0		 
				>;
				pinctrl-single,bias-pulldown = <0 2 0 2>;
				pinctrl-single,bias-pullup = <0 1 0 1>;
			};
			uart0_cfg_idle: uart0-cfg-idle-pins {
