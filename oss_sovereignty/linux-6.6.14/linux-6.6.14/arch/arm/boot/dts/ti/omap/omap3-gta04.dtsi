			OMAP3_CORE1_IOPAD(0x2130, PIN_OUTPUT | MUX_MODE4)  
			OMAP3_CORE1_IOPAD(0x2132, PIN_INPUT_PULLDOWN | MUX_MODE4)  
		>;
	};
	mcbsp1_pins: mcbsp1-pins {
		pinctrl-single,pins = <
			OMAP3_CORE1_IOPAD(0x218c, PIN_INPUT | MUX_MODE4)	 
			OMAP3_CORE1_IOPAD(0x218e, PIN_OUTPUT | MUX_MODE0)	 
