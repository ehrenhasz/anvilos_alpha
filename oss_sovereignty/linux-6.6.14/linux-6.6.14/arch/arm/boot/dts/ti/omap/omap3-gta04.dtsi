&gpmc {
	ranges = <0 0 0x30000000 0x1000000>;  
	nand@0,0 {
		compatible = "ti,omap2-nand";
		reg = <0 0 4>;  
		interrupt-parent = <&gpmc>;
		interrupts = <0 IRQ_TYPE_NONE>,  
			     <1 IRQ_TYPE_NONE>;	 
		ti,nand-ecc-opt = "ham1";
		rb-gpios = <&gpmc 0 GPIO_ACTIVE_HIGH>;  
		nand-bus-width = <16>;
		#address-cells = <1>;
		#size-cells = <1>;
		gpmc,device-width = <2>;
		gpmc,cs-on-ns = <0>;
		gpmc,cs-rd-off-ns = <44>;
		gpmc,cs-wr-off-ns = <44>;
		gpmc,adv-on-ns = <6>;
		gpmc,adv-rd-off-ns = <34>;
		gpmc,adv-wr-off-ns = <44>;
		gpmc,oe-off-ns = <54>;
		gpmc,we-off-ns = <40>;
		gpmc,access-ns = <64>;
		gpmc,rd-cycle-ns = <82>;
		gpmc,wr-cycle-ns = <82>;
		gpmc,wr-access-ns = <40>;
		gpmc,wr-data-mux-bus-ns = <0>;
		gpmc,sync-clk-ps = <0>;
		x-loader@0 {
			label = "X-Loader";
			reg = <0 0x80000>;
		};
		bootloaders@80000 {
			label = "U-Boot";
			reg = <0x80000 0x1c0000>;
		};
		bootloaders_env@240000 {
			label = "U-Boot Env";
			reg = <0x240000 0x40000>;
		};
		kernel@280000 {
			label = "Kernel";
			reg = <0x280000 0x600000>;
		};
		filesystem@880000 {
			label = "File System";
			reg = <0x880000 0>;	 
		};
	};
};
