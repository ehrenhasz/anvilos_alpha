		"", "", "", "",  
		"", "", "", "ADC_INT#",  
		"BG95_PWRKEY", "BG95_RESET";  
	line50-hog {
		gpio-hog;
		gpios = <50 0>;
		line-name = "USB0_VBUS_OC#";
		input;
	};
	line54-hog {
		gpio-hog;
