			AM64X_IOPAD(0x0270, PIN_INPUT, 0)  
		>;
	};
	main_mcan0_pins_default: main-mcan0-default-pins {
		pinctrl-single,pins = <
			AM64X_IOPAD(0x0254, PIN_INPUT, 0)  
