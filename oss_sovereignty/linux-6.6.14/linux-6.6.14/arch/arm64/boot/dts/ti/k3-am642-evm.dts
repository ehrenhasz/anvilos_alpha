	ospi0_pins_default: ospi0-default-pins {
		pinctrl-single,pins = <
			AM64X_IOPAD(0x0000, PIN_OUTPUT, 0)  
			AM64X_IOPAD(0x002c, PIN_OUTPUT, 0)  
