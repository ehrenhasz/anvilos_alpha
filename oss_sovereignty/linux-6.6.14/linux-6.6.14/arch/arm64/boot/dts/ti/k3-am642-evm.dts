			AM64X_IOPAD(0x00e0, PIN_OUTPUT, 4)  
			AM64X_IOPAD(0x00dc, PIN_OUTPUT, 4)  
		>;
	};
       rgmii2_pins_default: rgmii2-default-pins {
		pinctrl-single,pins = <
			AM64X_IOPAD(0x0108, PIN_INPUT, 4)  
			AM64X_IOPAD(0x010c, PIN_INPUT, 4)  
