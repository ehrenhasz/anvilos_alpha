	main_uart0_pins_default: main-uart0-default-pins {
		pinctrl-single,pins = <
			AM64X_IOPAD(0x0238, PIN_INPUT, 0)  
			AM64X_IOPAD(0x023c, PIN_OUTPUT, 0)  
			AM64X_IOPAD(0x0230, PIN_INPUT, 0)  
			AM64X_IOPAD(0x0234, PIN_OUTPUT, 0)  
		>;
