








if [ $
	echo "Usage: $0 [options] datasetâ€¦" >&2
	exit 16
fi

ret=0
for dataset; do
	case "$dataset" in
		-*)
			continue
			;;
		*)
			;;
	esac

	pool="${dataset%%/*}"

	case "$(@sbindir@/zpool list -Ho health "$pool")" in
		DEGRADED)
			ret=$(( ret | 4 ))
			;;
		FAULTED)
			awk '!/^([[:space:]]*
				ret=$(( ret | 8 ))
			;;
		"")
			
			ret=$(( ret | 8 ))
			;;
		*)
			;;
	esac
done

exit "$ret"
