", \"foo\");\n  ASSERT (result != NULL);\n  ASSERT (strcmp (result, \"foo\") == 0);\n  free (result);\n\n  result = xasprintf (\"%s%s\", \"foo\", \"bar\");\n  ASSERT (result != NULL);\n  ASSERT (strcmp (result, \"foobar\") == 0);\n  free (result);\n\n  result = my_xasprintf (\"%s%sbaz\", \"foo\", \"bar\");\n  ASSERT (result != NULL);\n  ASSERT (strcmp (result, \"foobarbaz\") == 0);\n  free (result);\n}\n\nint\nmain (_GL_UNUSED int argc, char *argv[])\n{\n  test_xvasprintf ();\n  test_xasprintf ();\n\n  return 0;\n}\n",
